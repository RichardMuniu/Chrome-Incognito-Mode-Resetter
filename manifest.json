{
    "manifest_version": 2,
    "name": "Incognito Mode Resetter",
    "version": "1.0.0",

    "browser_action": {
        "default_icon": "sneaky-guy.png",
        "default_popup": "popup.html"
    },

    "permissions": [
        "tabs",
        "browsingData"
    ],
        
    "incognito": "split"
}